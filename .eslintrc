{
  "extends": "airbnb-base",
  "rules": {
    "no-multi-assign": "off",
    "no-shadow": "off",
    "no-return-assign": 1,
    "arrow-parens": "off",
    "max-len": [
      "warn",
      {
        "code": 120
      }
    ],
    "comma-dangle": [
      "error",
      "never"
    ],
    "no-restricted-syntax": [
      "error",
      "WithStatement",
      "BinaryExpression[operator='in']"
    ],
    "indent": [
      "error",
      2
    ],
    "no-param-reassign": "off",
    "no-plusplus": "off",
    "no-underscore-dangle": "off"
  }
}